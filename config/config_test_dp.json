{
    "dataset_type": "DAVIS",
    "dataset_path": "./datasets/DAVIS-train/JPEGImages/480p",
    "data_extention": "jpg",
    "cache_data": true,
    "val_dataset_path": "./datasets/DAVIS-test/JPEGImages/480p/rollercoaster",
    "enable_line_nortify": true,

    "name": "naf_tsm_pt_b8",
    "model_type_train": "NAFTSM",
    "model_type_test": "NAFBBB",
    "n_gpu": 1,
    "batch_size": 8,
    "random_flip": true,
    "random_rotate_range": [0, 0],
    "steps": 400000,
    "use_grad_clip": true,
    "T_max": 400000,
    "eta_min": 1e-7,
    "learning_rate_G": 1e-3,
    "betas": [0.9, 0.9], 
    "sigma_range": [5, 55],

    "print_freq": 100,
    "eval_freq": 5000,
    "save_freq": 50000,
    "pretrained_path": null,

    "input_resolution": [96, 96],

    "color_channels": 3,
    "n_frames": 11,
    "tsm_fold": 8,
    "n_noise_channel": 1,
    "width": 32,
    "enc_blk_nums": [1, 1, 1], 
    "middle_blk_num": 3,
    "dec_blk_nums": [1, 1, 1]
}