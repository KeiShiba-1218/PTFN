{
    "dataset_path": "./datasets/DAVIS-train/JPEGImages/480p",
    "data_extention": "jpg",
    "val_dataset_path": "./datasets/DAVIS-test/JPEGImages/480p/rollercoaster",
    "enable_line_nortify": true,

    "name": "ptfn_progressive",
    "model_type_train": "PseudoTemporalFusionNetwork",
    "model_type_test": "PseudoTemporalFusionNetworkEval",

    "batch_size_list": [10,8,5,4,3,2,2],
    "input_resolution_list": [[96,96], [128,128], [192,192], [256,256], [320,320], [384,384], [480,480]],
    "n_frames_list": [10,7,5,3,3,3,2],
    "start_iter_list": [0, 100000, 150000, 200000, 250000, 300000, 350000], 

    "random_flip": true,
    "random_rotate_range": [0, 0],
    "steps": 400000,
    "use_grad_clip": true,
    "T_max": 400000,
    "eta_min": 1e-7,
    "learning_rate_G": 1e-3,
    "betas": [0.9, 0.9], 
    "sigma_range": [5, 55],

    "print_freq": 100,
    "eval_freq": 5000,
    "save_freq": 50000,
    "pretrained_path": null,
    "sigmas_for_eval": [50],

    "color_channels": 3,
    "tsm_fold": 8,
    "n_noise_channel": 1,
    "width": 32,
    "enc_blk_nums": [1, 1, 1], 
    "middle_blk_num": 3,
    "dec_blk_nums": [1, 1, 1]
}