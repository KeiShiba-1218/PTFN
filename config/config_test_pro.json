{
    "dataset_path": "./datasets/DAVIS-train/JPEGImages/480p",
    "data_extention": "jpg",
    "val_dataset_path": "./datasets/DAVIS-test/JPEGImages/480p/rollercoaster",
    "enable_line_nortify": true,

    "name": "ptfn_progressive3",
    "model_type_train": "PseudoTemporalFusionNetwork",
    "model_type_test": "PseudoTemporalFusionNetworkEval",

    "batch_size_list": [8,6],
    "input_resolution_list": [[96,96], [128,128]],
    "n_frames_list": [11,8],
    "start_iter_list": [0, 300000], 

    "random_flip": true,
    "random_rotate_range": [0, 0],
    "steps": 400000,
    "use_grad_clip": true,
    "T_max": 400000,
    "eta_min": 1e-7,
    "learning_rate_G": 1e-3,
    "betas": [0.9, 0.9], 
    "sigma_range": [5, 55],

    "print_freq": 100,
    "eval_freq": 5000,
    "save_freq": 50000,
    "pretrained_path": "experiments/ptfn_b8/ckpt/ptfn_b8_200000.ckpt",
    "resume_step": 200000,
    "create_new_log": true,
    "sigmas_for_eval": [50],

    "color_channels": 3,
    "tsm_fold": 8,
    "n_noise_channel": 1,
    "width": 32,
    "enc_blk_nums": [1, 1, 1], 
    "middle_blk_num": 3,
    "dec_blk_nums": [1, 1, 1]
}